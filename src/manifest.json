{
    "name": "Messenger Twitch Emotes",
    "version": "0.0.1",
    "description": "Twitch emotes in messenger",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}